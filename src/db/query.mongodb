
use("Bodegas_Mongo");
db.createCollection("Bodega", {
    validator: {
        $jsonSchema: {
            bsonType: "object",
            additionalProperties: false,
            required: ["nombre", "id_responsable", "estado", "created_at"],
            properties: {
                id: {
                    bsonType: "int",
                    description: "El ID es obligatorio y de tipo integer"
                },
                nombre: {
                    bsonType: "string",
                    description: "El nombre es obligatorio y de tipo string"
                },
                id_responsable: {
                    bsonType: "int",
                    description: "El ID del responsable es obligatorio y de tipo integer"
                },
                estado: {
                    bsonType: "int",
                    description: "El estado es obligatorio y de tipo integer"
                },
                created_by: {
                    bsonType: "int",
                    description: "El ID del creador es obligatorio y de tipo integer"
                },
                update_by: {
                    bsonType: "int",
                    description: "El ID del actualizador es opcional y de tipo integer"
                },
                created_at: {
                    bsonType: "date",
                    description: "La fecha de creación es obligatoria y de tipo date"
                },
                updated_at: {
                    bsonType: "date",
                    description: "La fecha de actualización es opcional y de tipo date o null"
                },
                deleted_at: {
                    bsonType: "date",
                    description: "La fecha de eliminación es opcional y de tipo date o null"
                }
            }
        }
    }
})

